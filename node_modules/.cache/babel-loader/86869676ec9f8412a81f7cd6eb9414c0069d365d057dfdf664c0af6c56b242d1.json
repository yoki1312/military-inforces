{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      username: this.$session.get(\"username\") ? this.$session.get(\"username\") : \"\",\n      soal: [],\n      perPage: 3,\n      currentPage: 1,\n      rows: 12\n    };\n  },\n  methods: {\n    setSoal(data) {\n      this.soal = data;\n    },\n    edit: function (id) {\n      this.$router.push({\n        path: '/admin/edit_soal/' + id\n      });\n    },\n    hapus: function (id) {\n      axios.post('https://bimbel-militaryinforces.com/api/admin/soal/do_delete_soal', {\n        id: id\n      }, {\n        headers: {\n          \"Content-type\": \"text/plain\"\n        }\n      }).then(response => {\n        // console.log(response.data)\n        const status = response.data.status;\n        const message = response.data.message;\n        alert(message);\n        if (status == 1) {\n          location.reload();\n        }\n      }).catch(error => console.log(error));\n    }\n  },\n  mounted() {\n    axios.get('https://bimbel-militaryinforces.com/api/admin/soal/all_soal', {\n      headers: {\n        \"Content-type\": \"application/json\"\n      }\n    }).then(response => {\n      console.log(response.data);\n      this.setSoal(response.data);\n    }).catch(error => console.log(error));\n  }\n};","map":{"version":3,"names":["axios","data","username","$session","get","soal","perPage","currentPage","rows","methods","setSoal","edit","id","$router","push","path","hapus","post","headers","then","response","status","message","alert","location","reload","catch","error","console","log","mounted"],"sources":["src/Admin/views/InputSoal.vue"],"sourcesContent":["<template>\r\n    <div class=\"container table-admin\">\r\n      <h4>Daftar Soal</h4>\r\n      <a href=\"/admin/add_soal\" class=\"tambah-data\">Tambah Soal</a>\r\n      <table class=\"table\">\r\n        <thead>\r\n          <tr>\r\n            <th>No</th>\r\n            <th>Nama Kategori</th>\r\n            <th>Nama Soal</th>\r\n            <th>Jawaban A</th>\r\n            <th>Jawaban B</th>\r\n            <th>Jawaban C</th>\r\n            <th>Jawaban D</th>\r\n            <!-- <th>Jawaban E</th> -->\r\n            <th>Jawaban Benar</th>\r\n            <th>Score</th>\r\n            <th>Opsi</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"(item, index) in soal.data\" :key=\"index\">\r\n            <td>{{ index+1 }}</td>\r\n            <td>{{ item.nama_category }}</td>\r\n            <td>{{ item.text_pertanyaan }}</td>\r\n            <td>{{ item.a }}</td>\r\n            <td>{{ item.b }}</td>\r\n            <td>{{ item.c }}</td>\r\n            <td>{{ item.d }}</td>\r\n            <!-- <td>{{ item.e }}</td> -->\r\n            <td>{{ item.kunci_jawaban }}</td>\r\n            <td>{{ item.score }}</td>\r\n            <td>\r\n                <button class=\"edit-opsi\" @click=\"() => edit(item.id)\"><i class=\"bx bx-edit\"></i></button>\r\n                <button class=\"delete-opsi\" @click=\"() => hapus(item.id)\"><i class=\"bx bx-trash\"></i></button>\r\n                <!-- <a href=\"\" class=\"edit-opsi\" @click=\"() => edit(item.id)\"><i class=\"bx bx-edit\"></i></a> -->\r\n                <!-- <a href=\"\" class=\"delete-opsi\"><i class=\"bx bx-trash\"></i></a> -->\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n    import axios from 'axios';\r\n\r\n    export default {\r\n      data(){\r\n        return{\r\n          username: this.$session.get(\"username\")?this.$session.get(\"username\"):\"\",\r\n          soal:[],\r\n          perPage: 3,\r\n        currentPage: 1,\r\n        rows: 12\r\n        }\r\n      },\r\n      methods:{\r\n        setSoal(data){\r\n          this.soal = data\r\n        },\r\n        edit: function (id) {\r\n          this.$router\r\n                    .push({ path: '/admin/edit_soal/'+ id})\r\n        },\r\n        hapus: function (id) {\r\n          axios.post('https://bimbel-militaryinforces.com/api/admin/soal/do_delete_soal', \r\n            {\r\n                  id : id,\r\n              },\r\n              {\r\n                headers: {\r\n                  \"Content-type\": \"text/plain\",\r\n                  }\r\n              })\r\n            .then( \r\n              (response) => {\r\n                  // console.log(response.data)\r\n                  const status = response.data.status\r\n                  const message = response.data.message\r\n                  \r\n                  alert(message)\r\n\r\n                  if (status == 1) {\r\n                      location.reload();\r\n                  } \r\n              }\r\n            )\r\n            .catch((error) => console.log(error));\r\n        }\r\n      },\r\n      mounted(){\r\n        axios.get('https://bimbel-militaryinforces.com/api/admin/soal/all_soal', {\r\n          headers: {\r\n            \"Content-type\": \"application/json\",\r\n            }\r\n        })\r\n      .then( (response) => {\r\n        console.log(response.data)\r\n        this.setSoal(response.data)\r\n      })\r\n      .catch((error) => console.log(error));\r\n      }\r\n    }\r\n  </script>\r\n  \r\n  <style>\r\n  .card-header {\r\n      font-size: 2rem;\r\n  }\r\n  </style>\r\n  "],"mappings":";AA6CA,OAAAA,KAAA;AAEA;EACAC,KAAA;IACA;MACAC,QAAA,OAAAC,QAAA,CAAAC,GAAA,oBAAAD,QAAA,CAAAC,GAAA;MACAC,IAAA;MACAC,OAAA;MACAC,WAAA;MACAC,IAAA;IACA;EACA;EACAC,OAAA;IACAC,QAAAT,IAAA;MACA,KAAAI,IAAA,GAAAJ,IAAA;IACA;IACAU,IAAA,WAAAA,CAAAC,EAAA;MACA,KAAAC,OAAA,CACAC,IAAA;QAAAC,IAAA,wBAAAH;MAAA;IACA;IACAI,KAAA,WAAAA,CAAAJ,EAAA;MACAZ,KAAA,CAAAiB,IAAA,sEACA;QACAL,EAAA,EAAAA;MACA,GACA;QACAM,OAAA;UACA;QACA;MACA,GACAC,IAAA,CACAC,QAAA;QACA;QACA,MAAAC,MAAA,GAAAD,QAAA,CAAAnB,IAAA,CAAAoB,MAAA;QACA,MAAAC,OAAA,GAAAF,QAAA,CAAAnB,IAAA,CAAAqB,OAAA;QAEAC,KAAA,CAAAD,OAAA;QAEA,IAAAD,MAAA;UACAG,QAAA,CAAAC,MAAA;QACA;MACA,CACA,EACA