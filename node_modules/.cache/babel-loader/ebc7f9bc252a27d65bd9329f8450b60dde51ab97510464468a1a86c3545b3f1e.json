{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      username: this.$session.get(\"username\") ? this.$session.get(\"username\") : \"\",\n      category: []\n    };\n  },\n  methods: {\n    setCategory(data) {\n      this.category = data;\n    },\n    edit: function (id) {\n      this.$router.push({\n        path: '/admin/edit_kategori/' + id\n      });\n    },\n    hapus: function (id) {\n      axios.post('https://bimbel-militaryinforces.com/api/admin/landing_page/do_delete_category', {\n        id: id\n      }, {\n        headers: {\n          \"Content-type\": \"text/plain\"\n        }\n      }).then(response => {\n        // console.log(response.data)\n        const status = response.data.status;\n        const message = response.data.message;\n        alert(message);\n        if (status == 1) {\n          location.reload();\n        }\n      }).catch(error => console.log(error));\n    }\n  },\n  mounted() {\n    axios.get('https://bimbel-militaryinforces.com/api/admin/landing_page/all_category', {\n      headers: {\n        \"Content-type\": \"application/json\"\n      }\n    }).then(response => {\n      console.log(response.data);\n      this.setCategory(response.data);\n    }).catch(error => console.log(error));\n  }\n};","map":{"version":3,"names":["axios","data","username","$session","get","category","methods","setCategory","edit","id","$router","push","path","hapus","post","headers","then","response","status","message","alert","location","reload","catch","error","console","log","mounted"],"sources":["src/Admin/views/InputKategori.vue"],"sourcesContent":["<template>\r\n    <div class=\"container table-admin\">\r\n      <h4>Daftar Nama Kategori soal</h4>\r\n      <a href=\"/admin/add_kategori\" class=\"tambah-data\">Tambah Kategori</a>\r\n      <table class=\"table\">\r\n        <thead>\r\n          <tr>\r\n            <th>No</th>\r\n            <th>Nama Kategori</th>\