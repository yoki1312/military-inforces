     axios.get('https://bimbel-militaryinforces.com/api/admin/landing_page/all_category', {\r\n          headers: {\r\n            \"Content-type\": \"application/json\",\r\n            }\r\n        })\r\n      .then( (response) => {\r\n        console.log(response.data)\r\n        this.setCategory(response.data)\r\n      })\r\n      .catch((error) => console.log(error));\r\n      }\r\n    }\r\n  </script>\r\n  \r\n  <style>\r\n  </style>\r\n  "],"mappings":";AA2BA,OAAAA,KAAA;AAEA;EACAC,KAAA;IACA;MACAC,QAAA,OAAAC,QAAA,CAAAC,GAAA,oBAAAD,QAAA,CAAAC,GAAA;MACAC,QAAA;IACA;EACA;EACAC,OAAA;IACAC,YAAAN,IAAA;MACA,KAAAI,QAAA,GAAAJ,IAAA;IACA;IACAO,IAAA,WAAAA,CAAAC,EAAA;MACA,KAAAC,OAAA,CACAC,IAAA;QAAAC,IAAA,4BAAAH;MAAA;IACA;IACAI,KAAA,WAAAA,CAAAJ,EAAA;MACAT,KAAA,CAAAc,IAAA,kFACA;QACAL,EAAA,EAAAA;MACA,GACA;QACAM,OAAA;UACA;QACA;MACA,GACAC,IAAA,CACAC,QAAA;QACA;QACA,MAAAC,MAAA,GAAAD,QAAA,CAAAhB,IAAA,CAAAiB,MAAA;QACA,MAAAC,OAAA,GAAAF,QAAA,CAAAhB,IAAA,CAAAkB,OAAA;QAEAC,KAAA,CAAAD,OAAA;QAEA,IAAAD,MAAA;UACAG,QAAA,CAAAC,MAAA;QACA;MACA,CACA,EACAC,KAAA,CAAAC,KAAA,IAAAC,OAAA,CAAAC,GAAA,CAAAF,KAAA;IACA;EACA;EACAG,QAAA;IACA3B,KAAA,CAAAI,GAAA;MACAW,OAAA;QACA;MACA;IACA,GACAC,IAAA,CAAAC,QAAA;MACAQ,OAAA,CAAAC,GAAA,CAAAT,QAAA,CAAAhB,IAAA;MACA,KAAAM,WAAA,CAAAU,QAAA,CAAAhB,IAAA;IACA,GACAsB,KAAA,CAAAC,KAAA,IAAAC,OAAA,CAAAC,GAAA,CAAAF,KAAA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}