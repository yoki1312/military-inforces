{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\n// @ is an alias to /src\nimport Header from '@/components/landing_page/Header.vue';\nimport Navbar from '@/components/landing_page/Navbar.vue';\nimport Footer from '@/components/landing_page/Footer.vue';\nimport CoolLightBox from 'vue-cool-lightbox';\nimport 'vue-cool-lightbox/dist/vue-cool-lightbox.min.css';\nimport axios from 'axios';\nexport default {\n  name: 'FotoKegiatan',\n  components: {\n    Header,\n    Navbar,\n    Footer,\n    CoolLightBox\n  },\n  data() {\n    return {\n      items: [{\n        text: 'Beranda',\n        href: '/'\n      }, {\n        text: 'Foto Kegiatan',\n        active: true\n      }],\n      foto: [],\n      index: null\n    };\n  },\n  mounted() {\n    // Ganti URL di bawah dengan URL sebenarnya dari server Anda\n    const apiUrl = 'http://site.bimbel-militaryinforces.com/api/kegiatan';\n\n    // Menggunakan Axios untuk mengambil data\n    axios.get(apiUrl).then(response => {\n      console.log(response.data.data);\n      var results = response.data.data;\n      var foto = [];\n      for (let index = 0; index < results.length; index++) {\n        const element = results[index];\n        foto.push(element.url_gambar);\n      }\n      this.foto = foto;\n      // Memperbarui data 'foto' dengan data yang diterima dari server\n    }).catch(error => {\n      console.error('Error fetching data:', error);\n    });\n  }\n};","map":{"version":3,"names":["Header","Navbar","Footer","CoolLightBox","axios","name","components","data","items","text","href","active","foto","index","mounted","apiUrl","get","then","response","console","log","results","length","element","push","url_gambar","catch","error"],"sources":["src/views/landing_page/FotoKegiatan.vue"],"sourcesContent":["<template>\r\n  <div class=\"foto-kegiatan\">\r\n    <Header />\r\n    <Navbar />\r\n\r\n    <section class=\"head-breadcrumb\">\r\n      <div class=\"container\">\r\n        <div class=\"row\">\r\n          <div class=\"col-xl-12 col-lg-12 col-md-12 col-sm-12\">\r\n            <b-breadcrumb :items=\"items\"></b-breadcrumb>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n    <section class=\"body-skedinasan\">\r\n      <div class=\"container\">\r\n        <div class=\"row\">\r\n          <div class=\"col-xl-12 col-lg-12 col-md-12 col-sm-12\">\r\n            <h1>Geleri FOTO KEGIATAN</h1>\r\n          </div>\r\n          <div class=\"col-xl-12 col-lg-12 col-md-12 col-sm-12\">\r\n            <CoolLightBox \r\n              :items=\"foto\" \r\n              :index=\"index\"\r\n              @close=\"index = null\">\r\n            </CoolLightBox>\r\n\r\n            <div class=\"images-wrapper\">\r\n              <div class=\"row\">\r\n                <div  v-for=\"(image, imageIndex) in foto\" :key=\"imageIndex\" class=\"col-sm-3\">\r\n                    <div class=\"image\" @click=\"index = image\" :style=\"{ backgroundImage: 'url(' + image + ')' }\">\r\n                      <!-- Tambahkan teks di bawah setiap foto -->\r\n                      <div class=\"image-text\">Deskripsi Foto</div>\r\n                  </div>\r\n\r\n                </div>\r\n              </div>\r\n            </div>\r\n\r\n\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n\r\n    <Footer />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n// @ is an alias to /src\r\nimport Header from '@/components/landing_page/Header.vue'\r\nimport Navbar from '@/components/landing_page/Navbar.vue'\r\nimport Footer from '@/components/landing_page/Footer.vue'\r\n\r\nimport CoolLightBox from 'vue-cool-lightbox'\r\nimport 'vue-cool-lightbox/dist/vue-cool-lightbox.min.css'\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'FotoKegiatan',\r\n  components: {\r\n    Header,\r\n    Navbar,\r\n    Footer,\r\n    CoolLightBox,\r\n  },\r\n  data() {\r\n    return {\r\n      items: [\r\n        {\r\n          text: 'Beranda',\r\n          href: '/'\r\n        },\r\n        {\r\n          text: 'Foto Kegiatan',\r\n          active: true\r\n        }\r\n      ],\r\n      foto: [],\r\n      index: null\r\n    }\r\n  },\r\n  mounted() {\r\n    // Ganti URL di bawah dengan URL sebenarnya dari server Anda\r\n    const apiUrl = 'http://site.bimbel-militaryinforces.com/api/kegiatan';\r\n\r\n    // Menggunakan Axios untuk mengambil data\r\n    axios.get(apiUrl)\r\n      .then(response => {\r\n        console.log(response.data.data);\r\n        var results = response.data.data;\r\n        var foto = [];\r\n        for (let index = 0; index < results.length; index++) {\r\n          const element = results[index];\r\n          foto.push(element.url_gambar);\r\n        }\r\n        this.foto = foto;\r\n        // Memperbarui data 'foto' dengan data yang diterima dari server\r\n      })\r\n      .catch(error => {\r\n        console.error('Error fetching data:', error);\r\n      });\r\n  }\r\n}\r\n</script>\r\n"],"mappings":";AAkDA;AACA,OAAAA,MAAA;AACA,OAAAC,MAAA;AACA,OAAAC,MAAA;AAEA,OAAAC,YAAA;AACA;AACA,OAAAC,KAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IACAN,MAAA;IACAC,MAAA;IACAC,MAAA;IACAC;EACA;EACAI,KAAA;IACA;MACAC,KAAA,GACA;QACAC,IAAA;QACAC,IAAA;MACA,GACA;QACAD,IAAA;QACAE,MAAA;MACA,EACA;MACAC,IAAA;MACAC,KAAA;IACA;EACA;EACAC,QAAA;IACA;IACA,MAAAC,MAAA;;IAEA;IACAX,KAAA,CAAAY,GAAA,CAAAD,MAAA,EACAE,IAAA,CAAAC,QAAA;MACAC,OAAA,CAAAC,GAAA,CAAAF,QAAA,CAAAX,IAAA,CAAAA,IAAA;MACA,IAAAc,OAAA,GAAAH,QAAA,CAAAX,IAAA,CAAAA,IAAA;MACA,IAAAK,IAAA;MACA,SAAAC,KAAA,MAAAA,KAAA,GAAAQ,OAAA,CAAAC,MAAA,EAAAT,KAAA;QACA,MAAAU,OAAA,GAAAF,OAAA,CAAAR,KAAA;QACAD,IAAA,CAAAY,IAAA,CAAAD,OAAA,CAAAE,UAAA;MACA;MACA,KAAAb,IAAA,GAAAA,IAAA;MACA;IACA,GACAc,KAAA,CAAAC,KAAA;MACAR,OAAA,CAAAQ,KAAA,yBAAAA,KAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}