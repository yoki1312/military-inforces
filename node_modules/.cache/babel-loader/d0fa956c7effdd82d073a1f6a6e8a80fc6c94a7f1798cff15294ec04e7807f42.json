{"ast":null,"code":"// @ is an alias to /src\nimport Header from '@/components/landing_page/Header.vue';\nimport Navbar from '@/components/landing_page/Navbar.vue';\nimport Footer from '@/components/landing_page/Footer.vue';\nimport axios from 'axios';\nexport default {\n  name: 'SekolahKedinasan',\n  components: {\n    Header,\n    Navbar,\n    Footer\n  },\n  data() {\n    return {\n      items: [{\n        text: 'Beranda',\n        href: '/'\n      }, {\n        text: 'Sekolah Kedinasan',\n        active: true\n      }],\n      foto: []\n    };\n  },\n  mounted() {\n    // Ganti URL di bawah dengan URL sebenarnya dari server Anda\n    const apiUrl = 'https://site.bimbel-militaryinforces.com/api/sekolahKedinasanApi';\n\n    // Menggunakan Axios untuk mengambil data\n    axios.get(apiUrl).then(response => {\n      this.foto = response.data.data;\n      // Memperbarui data 'foto' dengan data yang diterima dari server\n    }).catch(error => {\n      console.error('Error fetching data:', error);\n    });\n  }\n};","map":{"version":3,"names":["Header","Navbar","Footer","axios","name","components","data","items","text","href","active","foto","mounted","apiUrl","get","then","response","catch","error","console"],"sources":["src/views/landing_page/SekolahKedinasan.vue"],"sourcesContent":["<template>\r\n  <div class=\"sekolah-kedinasan\">\r\n    <Header />\r\n    <Navbar />\r\n\r\n    <section class=\"head-breadcrumb\">\r\n      <div class=\"container\">\r\n        <div class=\"row\">\r\n          <div class=\"col-xl-12 col-lg-12 col-md-12 col-sm-12\">\r\n            <b-breadcrumb :items=\"items\"></b-breadcrumb>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n    <section class=\"body-skedinasan\">\r\n      <div class=\"container\">\r\n        .row\r\n        <div v-for=\"(image, imageIndex) in foto\" :key=\"imageIndex\" class=\"col-sm-3 form-group\">\r\n          <div class=\"card\" @click=\"index = image\">\r\n            <img :src=\"image.url_gambar\" class=\"card-img-top\" style=\"max-height: 300px ;min-height: 300px ;\">\r\n            <div class=\"card-body\">\r\n              <br>\r\n              <h5 class=\"card-title\">{{ image.nama_kegiatan }}</h5>\r\n              <p class=\"card-text\">{{ image.deskripsi }}</p>\r\n            </div>\r\n          </div>\r\n\r\n        </div>\r\n      </div>\r\n    </section>\r\n\r\n    <Footer />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  // @ is an alias to /src\r\n  import Header from '@/components/landing_page/Header.vue'\r\n  import Navbar from '@/components/landing_page/Navbar.vue'\r\n  import Footer from '@/components/landing_page/Footer.vue'\r\n  import axios from 'axios';\r\n\r\n\r\n  export default {\r\n    name: 'SekolahKedinasan',\r\n    components: {\r\n      Header,\r\n      Navbar,\r\n      Footer\r\n    },\r\n    data() {\r\n      return {\r\n\r\n        items: [{\r\n            text: 'Beranda',\r\n            href: '/'\r\n          },\r\n          {\r\n            text: 'Sekolah Kedinasan',\r\n            active: true\r\n          }\r\n        ],\r\n        foto: [],\r\n      }\r\n    },\r\n    mounted() {\r\n      // Ganti URL di bawah dengan URL sebenarnya dari server Anda\r\n      const apiUrl = 'https://site.bimbel-militaryinforces.com/api/sekolahKedinasanApi';\r\n\r\n      // Menggunakan Axios untuk mengambil data\r\n      axios.get(apiUrl)\r\n        .then(response => {\r\n          this.foto = response.data.data;\r\n          // Memperbarui data 'foto' dengan data yang diterima dari server\r\n        })\r\n        .catch(error => {\r\n          console.error('Error fetching data:', error);\r\n        });\r\n    }\r\n  }\r\n</script>"],"mappings":"AAoCA;AACA,OAAAA,MAAA;AACA,OAAAC,MAAA;AACA,OAAAC,MAAA;AACA,OAAAC,KAAA;AAGA;EACAC,IAAA;EACAC,UAAA;IACAL,MAAA;IACAC,MAAA;IACAC;EACA;EACAI,KAAA;IACA;MAEAC,KAAA;QACAC,IAAA;QACAC,IAAA;MACA,GACA;QACAD,IAAA;QACAE,MAAA;MACA,EACA;MACAC,IAAA;IACA;EACA;EACAC,QAAA;IACA;IACA,MAAAC,MAAA;;IAEA;IACAV,KAAA,CA