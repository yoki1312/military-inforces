{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      username: this.$session.get(\"username\") ? this.$session.get(\"username\") : \"\",\n      member: []\n    };\n  },\n  methods: {\n    setMember(data) {\n      this.member = data;\n    },\n    edit: function (id) {\n      this.$router.push({\n        path: '/admin/edit_harga_bimbingan/' + id\n      });\n    },\n    hapus: function (id) {\n      axios.post('https://bimbel-militaryinforces.com/api/admin/landing_page/do_delete_paket', {\n        id: id\n      }, {\n        headers: {\n          'Content-Type': 'multipart/form-data'\n        }\n      }).then(response => {\n        // console.log(response.data)\n        const status = response.data.status;\n        const message = response.data.message;\n        alert(message);\n        if (status == 1) {\n          location.reload();\n        }\n      }).catch(error => console.log(error));\n    }\n  },\n  mounted() {\n    axios.get('https://bimbel-militaryinforces.com/api/admin/landing_page/all_paket', {\n      headers: {\n        \"Content-type\": \"application/json\"\n      }\n    }).then(response => {\n      console.log(response.data);\n      this.setMember(response.data);\n    }).catch(error => console.log(error));\n  }\n};","map":{"version":3,"names":["axios","data","username","$session","get","member","methods","setMember","edit","id","$router","push","path","hapus","post","headers","then","response","status","message","alert","location","reload","catch","error","console","log","mounted"],"sources":["src/Admin/views/InputHargaBimbingan.vue"],"sourcesContent":["<template>\r\n    <div class=\"container table-admin\">\r\n      <h4>Daftar Harga Bimbingan</h4>\r\n      <a href=\"/admin/add_harga_bimbingan\" class=\"tambah-data\">Tambah Bimbingan</a>\r\n      <table class=\"table\">\r\n        <thead>\r\n          <tr>\r\n            <th>No</th>\r\n            <th>Nama Bimbingan</th>\r\n            <th>Harga</th>\r\n            <th>Deskripsi</th>\r\n            <th>Image</th>\r\n            <th>List</th>\r\n            <th>Opsi</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <!-- {{history.data}}   -->\r\n          <tr v-for=\"(item, index) in member.data\" :key=\"index\">\r\n            <td>{{ index+1 }}</td>\r\n            <td>{{ item.nama_paket }}</td>\r\n            <td>{{ item.harga }}</td>\r\n            <td>{{ item.deskripsi }}</td>\r\n            <td>{{ item.path_foto }}</td>\r\n            <td>{{ item.list_deskripsi_paket }}</td>\r\n            <td>\r\n                <button class=\"edit-opsi\" @click=\"() => edit(item.id)\"><i class=\"bx bx-edit\"></i></button>\r\n                <button  @click=\"() => hapus(item.id)\" class=\"delete-opsi\"><i class=\"bx bx-trash\"></i></button>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n  </template>\r\n  \r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\ndata(){\r\n    return{\r\n      username: this.$session.get(\"username\")?this.$session.get(\"username\"):\"\",\r\n      member:[],\r\n    }\r\n  },\r\n  methods:{\r\n      setMember(data){\r\n        this.member = data\r\n      },\r\n      edit: function (id) {\r\n        this.$router\r\n                  .push({ path: '/admin/edit_harga_bimbingan/'+ id})\r\n      },\r\n\r\n      hapus: function (id) {\r\n        axios.post('https://bimbel-militaryinforces.com/api/admin/landing_page/do_delete_paket', \r\n          {\r\n            id : id,\r\n          },\r\n          {\r\n            headers: {\r\n              'Content-Type': 'multipart/form-data',\r\n            }\r\n          })\r\n          .then( \r\n            (response) => {\r\n                // console.log(response.data)\r\n                const status = response.data.status\r\n                const message = response.data.message\r\n                \r\n                alert(message)\r\n\r\n                if (status == 1) {\r\n                    location.reload();\r\n                } \r\n            }\r\n          )\r\n          .catch((error) => console.log(error));\r\n      }\r\n  },\r\n  mounted(){\r\n    axios.get('https://bimbel-militaryinforces.com/api/admin/landing_page/all_paket', {\r\n      headers: {\r\n        \"Content-type\": \"application/json\",\r\n        }\r\n    })\r\n    .then( (response) => {\r\n      console.log(response.data)\r\n      this.setMember(response.data)\r\n    })\r\n    .catch((error) => console.log(error));\r\n  }\r\n}\r\n</script>\r\n  \r\n  <style>\r\n  .card-header {\r\n      font-size: 2rem;\r\n  }\r\n  </style>\r\n  "],"mappings":";AAoCA,OAAAA,KAAA;AAEA;EACAC,KAAA;IACA;MACAC,QAAA,OAAAC,QAAA,CAAAC,GAAA,oBAAAD,QAAA,CAAAC,GAAA;MACAC,MAAA;IACA;EACA;EACAC,OAAA;IACAC,UAAAN,IAAA;MACA,KAAAI,MAAA,GAAAJ,IAAA;IACA;IACAO,IAAA,WAAAA,CAAAC,EAAA;MACA,KAAAC,OAAA,CACAC,IAAA;QAAAC,IAAA,mCAAAH;MAAA;IACA;IAEAI,KAAA,WAAAA,CAAAJ,EAAA;MACAT,KAAA,CAAAc,IAAA,+EACA;QACAL,EAAA,EAAAA;MACA,GACA;QACAM,OAAA;UACA;QACA;MACA,GACAC,IAAA,CACAC,QAAA;QACA;QACA,MAAAC,MAAA,GAAAD,QAAA,CAAAhB,IAAA,CAAAiB,MAAA;QACA,MAAAC,OAAA,GAAAF,QAAA,CAAAhB,IAAA,CAAAkB,OAAA;QAEAC,KAAA,CAAAD,OAAA;QAEA,IAAAD,MAAA;UACAG,QAAA,CAAAC,MAAA;QACA;MACA,CACA,EACAC,KAAA,CAAAC,KAAA,IAAAC,OAAA,CAAAC,GAAA,CAAAF,KAAA;IACA;EACA;EACAG,QAAA;IACA3B,KAAA,CAAAI,GAAA;MACAW,OAAA;QACA;MACA;IACA,GACAC,IAAA,CAAAC,QAAA;MACAQ,OAAA,CAAAC,GAAA,CAAAT,QAAA,CAAAhB,IAAA;MACA,KAAAM,SAAA,CAAAU,QAAA,CAAAhB,IAAA;IACA,GACAsB,KAAA,CAAAC,KAAA,IAAAC,OAAA,CAAAC,GAAA,CAAAF,KAAA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}